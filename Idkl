local LEFT_OFFSET     = 60   
local MIN_HEIGHT      = 70
local TITLE_HEIGHT    = 20
local DESC_TOP        = 30 
local BOTTOM_PADDING  = 12
local BAR_HEIGHT      = 4

-- === GUI ROOT ===
