	TweenService:Create(teleportBtn, tweenInfo, {Size = hoverSize, Position = hoverPos}):Play()
end)

teleportBtn.MouseLeave:Connect(function()
	TweenService:Create(teleportBtn, tweenInfo, {Size = normalSize, Position = normalPos}):Play()
end)

teleportBtn.MouseButton1Click:Connect(function()
	local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local placeId = 110409866876187 -- thay b·∫±ng ID game c·ªßa b·∫°n

local success, err = pcall(function()
	TeleportService:Teleport(placeId, Players.LocalPlayer)
end)

if not success then
	warn("Teleport l·ªói:", err)
end
end)

