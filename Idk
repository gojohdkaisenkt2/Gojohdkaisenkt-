	Position = UDim2.new(0.5, 0, 0.4, 0)
}):Play()
task.delay(1.4, function()
	TweenService:Create(ball, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
		Position = UDim2.new(0.5, 0, 0.5, 0)
	}):Play()
end)
task.delay(2.2, function()
	local frame = Instance.new("Frame")
	frame.Size = UDim2.new(0, 0, 0, 0)
	frame.Position = UDim2.new(0.5, 0, 0.5, 0)
	frame.AnchorPoint = Vector2.new(0.5, 0.5)
	frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	frame.Parent = gui
	Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 20)

	local nameLabel = Instance.new("TextLabel", frame)
	nameLabel.Size = UDim2.new(1, -20, 0, 30)
	nameLabel.Position = UDim2.new(0.5, 0, 0.9, 0)
	nameLabel.AnchorPoint = Vector2.new(0.5, 1)

	nameLabel.BackgroundTransparency = 1
	nameLabel.Font = Enum.Font.GothamBold
	nameLabel.Text = "Luau x SS"
	nameLabel.TextColor3 = Color3.new(1, 1, 1)
	nameLabel.TextScaled = true
	local frameImage = image:Clone()
	frameImage.Size = UDim2.new(0, 200, 0, 150)
	frameImage.Position = UDim2.new(0.5, 0, 0.4, 0)
	frameImage.Parent = frame

	TweenService:Create(ball, TweenInfo.new(0.3), {
		Size = UDim2.new(0, 0, 0, 0),
		BackgroundTransparency = 1
	}):Play()
	game.Debris:AddItem(ball, 1)
	frame.Visible = true

	TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
		Size = UDim2.new(0, 320, 0, 220)
	}):Play()
	task.delay(3, function()
		TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.In), {
			Size = UDim2.new(0, 0, 0, 0)
		}):Play()
		task.wait(0.5)
		frame:Destroy()
	end)
end)
