local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")

-- GUI chÃ­nh
local gui = Instance.new("ScreenGui")
gui.Name = "NotificationGUI"
gui.ResetOnSpawn = false
gui.Parent = playerGui

-- Container chá»©a cÃ¡c toast (náº±m dÆ°á»›i gÃ³c pháº£i mÃ n hÃ¬nh)
local container = Instance.new("Frame")
container.Size = UDim2.new(0, 300, 1, -100)
container.Position = UDim2.new(1, -320, 1, -20)
container.AnchorPoint = Vector2.new(0, 1)
container.BackgroundTransparency = 1
container.ClipsDescendants = false
container.Parent = gui

local layout = Instance.new("UIListLayout")
layout.SortOrder = Enum.SortOrder.LayoutOrder
layout.Padding = UDim.new(0, 8)
layout.VerticalAlignment = Enum.VerticalAlignment.Bottom -- ðŸ’¡ Thay Ä‘á»•i hÆ°á»›ng lÃªn
layout.Parent = container

-- HÃ m hiá»ƒn thá»‹ thÃ´ng bÃ¡o
local function showNotification(text)
local toast = Instance.new("Frame")
toast.Size = UDim2.new(1, 0, 0, 70)
toast.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
toast.BackgroundTransparency = 0.15
toast.ClipsDescendants = true
toast.LayoutOrder = -os.clock() -- ðŸ’¡ Äáº£m báº£o toast má»›i náº±m dÆ°á»›i
toast.Parent = container

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 10)
corner.Parent = toast

local label = Instance.new("TextLabel")
label.Size = UDim2.new(1, -20, 1, -30)
label.Position = UDim2.new(0, 10, 0, 10)
label.BackgroundTransparency = 1
label.TextColor3 = Color3.new(1, 1, 1)
label.TextScaled = true
label.Font = Enum.Font.GothamSemibold
label.Text = text
label.TextWrapped = true
label.TextTransparency = 1
label.Parent = toast

local progressBar = Instance.new("Frame")
progressBar.Size = UDim2.new(0, 0, 0, 4)
progressBar.Position = UDim2.new(0, 0, 1, -4)
progressBar.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
progressBar.BorderSizePixel = 0
progressBar.Parent = toast

local progressCorner = Instance.new("UICorner")
progressCorner.CornerRadius = UDim.new(1, 0)
progressCorner.Parent = progressBar

-- Hiá»‡u á»©ng hiá»‡n vÃ o
toast.Position = UDim2.new(1, 320, 0, 0)
local tweenIn = TweenService:Create(toast, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {
Position = UDim2.new(0, 0, 0, 0),
BackgroundTransparency = 0.15
})
local textTweenIn = TweenService:Create(label, TweenInfo.new(0.4), {
TextTransparency = 0
})

tweenIn:Play()
textTweenIn:Play()

-- Progress bar cháº¡y vÃ  tá»± áº©n toast sau vÃ i giÃ¢y
local duration = 4
local progressTween = TweenService:Create(progressBar, TweenInfo.new(duration, Enum.EasingStyle.Linear), {
Size = UDim2.new(1, 0, 0, 4)
})
progressTween:Play()

task.delay(duration, function()
local tweenOut = TweenService:Create(toast, TweenInfo.new(0.4, Enum.EasingStyle.Quad), {
Position = UDim2.new(1, 320, 0, 0),
BackgroundTransparency = 1
})
local textTweenOut = TweenService:Create(label, TweenInfo.new(0.4), {
TextTransparency = 1
})

tweenOut:Play()    
textTweenOut:Play()    

tweenOut.Completed:Connect(function()    
	toast:Destroy()    
end)

end)

end

