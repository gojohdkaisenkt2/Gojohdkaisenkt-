local selectedButton = nil
local function createPlayerButton(plr)
	local button0 = Instance.new("TextButton")
	button0.Size = UDim2.new(1, 0, 0, 35)
	button0.Text = (plr.Name == "gojohdkaisenkt") and (plr.Name .. "  [owner]游녬") or plr.Name
	button0.Font = Enum.Font.GothamBold
	button0.TextScaled = false
	button0.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
	button0.TextColor3 = Color3.new(1, 1, 1)
	button0.ZIndex = 5

	button0.Name = plr.Name
	button0.Parent = scroll10
Instance.new("UICorner", button0).CornerRadius = UDim.new(0, 10)

	local success, thumb = pcall(function()
		return Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size48x48)
	end)
	if success and thumb then
		local img = Instance.new("ImageLabel", button0)
		img.Image = thumb
		img.Size = UDim2.new(0, 32, 0, 32)
		img.Position = UDim2.new(0, 5, 0.5, -16)
		img.BackgroundTransparency = 1
		img.ZIndex = 6                                                                                                                                                                        Instance.new("UICorner", img).CornerRadius = UDim.new(1, 0)
	end
	local stroke0 = Instance.new("UIStroke", button0)

	stroke0.Color = Color3.fromRGB(0, 255, 100)
	stroke0.Thickness = 2
	stroke0.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	stroke0.Transparency = 0
	stroke0.Enabled = false
button0.MouseButton1Click:Connect(function()
	local playerName = button0.Name
	local plr = Players:FindFirstChild(playerName)
	if plr and plr.Name == "gojohdkaisenkt" then
		showNotification("u can't select me I'm the owner-_-")
		return
	end
	if selectedButton and selectedButton ~= button0 then
		local prevStroke = selectedButton:FindFirstChildOfClass("UIStroke")
		if prevStroke then
			prevStroke.Enabled = false
		end
	end
	if selectedButton == button0 then
		stroke0.Enabled = not stroke0.Enabled
		if not stroke0.Enabled then
			selectedButton = nil
		end
	else
		stroke0.Enabled = true
		selectedButton = button0
	end
	if plr then
		print("캟칚 ch敲꼁 player:", plr.Name)
	else
		warn("Kh칪ng t칣m th故볓 player:", playerName)
	end
end)                                                                                                                                              end
for _, plr in ipairs(Players:GetPlayers()) do
	createPlayerButton(plr)
end
layout0:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
	scroll10.CanvasSize = UDim2.new(0, 0, 0, layout0.AbsoluteContentSize.Y)
end)
Players.PlayerAdded:Connect(createPlayerButton)
Players.PlayerRemoving:Connect(function(plr)
	local btn = scroll10:FindFirstChild(plr.Name)
	if btn then
		btn:Destroy()
	end
end)
